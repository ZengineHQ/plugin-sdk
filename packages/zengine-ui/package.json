{
  "name": "@zenginehq/zengine-ui",
  "version": "0.2.5",
  "description": "Zengine UI Styles",
  "author": "Alex Weber <alex.weber@wizehive.com>",
  "homepage": "https://github.com/ZengineHQ/plugin-sdk#readme",
  "scripts": {
    "start": "npm run sass -- --watch",
    "watch": "npm run sass -- --watch",
    "sass": "sass scss/style.scss lib/style.css",
    "sass:min": "sass scss/style.scss lib/style.min.css --style compressed",
    "build": "rimraf lib; npm run sass; npm run sass:min; cpy scss lib --parents; cpy package.json README.md CHANGELOG.md lib",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "main": "style.css",
  "files": [
    "style.css",
    "style.min.css",
    "scss"
  ],
  "sideEffects": false,
  "keywords": [
    "zengine",
    "zengine-ui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZengineHQ/plugin-sdk.git"
  },
  "devDependencies": {
    "bootstrap": "^4.4.1",
    "cpy-cli": "^3.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3"
  },
  "publishConfig": {
    "access": "public",
    "directory": "lib"
  },
  "gitHead": "fa063d893a275e1c69294529106d9677a2a885ef"
}
