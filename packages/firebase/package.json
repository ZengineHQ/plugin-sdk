{
  "name": "@zenginehq/firebase",
  "version": "0.1.1-alpha.0",
  "description": "Firebase wrapper for Zengine Plugins",
  "author": "Alex Weber <alex.weber@wizehive.com>",
  "homepage": "https://github.com/ZengineHQ/plugin-sdk#readme",
  "license": "MIT",
  "scripts": {
    "watch": "rimraf lib; concurrently \"npm run build:js -- --watch\" \"tsc --emitDeclarationOnly -w\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "CI=true npm test -- --collect-coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "build": "rimraf lib; npm run build:types; npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "firebase": "^2.3.2",
    "tslib": "^2.0.1"
  },
  "main": "lib/index.js",
  "sideEffects": false,
  "peerDependencies": {
    "@zenginehq/react-sdk": "^0.1.11",
    "react": "^16.13.1"
  },
  "keywords": [
    "zengine",
    "plugin",
    "react",
    "firebase"
  ],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "CHANGELOG.md",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZengineHQ/plugin-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/ZengineHQ/plugin-sdk/issues"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "@zenginehq/react-sdk": "^0.1.11",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.7.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.0",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-resolve": "^26.4.0",
    "jest-watch-typeahead": "^0.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.0.0",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7"
  }
}
