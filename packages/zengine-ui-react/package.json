  "homepage": "https://github.com/ZengineHQ/plugin-sdk#readme",
  "scripts": {
    "watch": "rimraf lib; tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*",
    "build": "rimraf lib; npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "prepublishOnly": "npm run build",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public --quiet",
    "docs": "npm run build-storybook && storybook-to-ghpages"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "sideEffects": false,
