    "tslib": "^2.0.0"
  },
  "keywords": [
    "zengine",
    "plugin",
    "react"
  ],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "CHANGELOG.md",
    "lib"
  ],
  "repository": {
    "type": "git",
