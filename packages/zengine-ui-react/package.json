{
  "name": "@zenginehq/zengine-ui-react",
  "version": "0.8.0",
  "description": "Zengine UI React",
  "author": "Alex Weber <alex.weber@wizehive.com>",
  "homepage": "https://github.com/ZengineHQ/plugin-sdk#readme",
  "scripts": {
    "watch": "rimraf lib; concurrently \"npm run build:js -- --watch\" \"tsc --emitDeclarationOnly -w\"",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "CI=true npm test -- --collect-coverage",
    "test:save": "CI=true npm run test -- --json --outputFile=./public/test-results.json",
    "lint": "eslint src/**/*",
    "build": "rimraf lib; npm run build:types; npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "prepublishOnly": "npm run build",
    "storybook:build": "build-storybook -s public --quiet;",
    "storybook:publish": "storybook-to-ghpages",
    "postpublish": "npm run test:save; npm run storybook:build; npm run storybook:publish"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "sideEffects": false,
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "formik": "^2.1.4",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react-bootstrap": "^1.0.1",
    "react-datepicker": "^2.16.0",
    "tslib": "^2.0.0"
  },
  "keywords": [
    "zengine",
    "plugin",
    "react"
  ],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "CHANGELOG.md",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZengineHQ/plugin-sdk.git"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
